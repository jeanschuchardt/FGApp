<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewData["Title"] - FGApp</title>

    <environment include="Development">
        <!-- Bootstrap core CSS-->
        <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css" />
        <!-- Custom fonts-->
        <link href="~/lib/fontawesome-free/css/all.css" rel="stylesheet" type="text/css">
        <!-- Page level plugin CSS-->
        <link href="~/lib/datatables/dataTables.bootstrap4.css" rel="stylesheet">
        <!-- Custom template styles-->
        <link href="~/css/sb-admin.css" rel="stylesheet">
    </environment>

    <environment include="Development">
        <!-- Bootstrap core JavaScript-->
        <script src="~/lib/jquery/jquery.js"></script>
        <script src="~/lib/bootstrap/js/bootstrap.js"></script>
        <!-- Core plugin JavaScript-->
        <script src="~/lib/jquery-easing/jquery.easing.js"></script>
        <!-- Custom scripts for all pages-->
        <script src="~/js/sb-admin.js"></script>
    </environment>

    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
</head>
<body id="page-top">
    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">
        <a class="navbar-brand mr-1" href="index.html">FG App</a>
        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button>

        <div class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
        </div>

        <!-- Navbar -->
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown no-arrow mx-1">
                <a class="nav-link dropdown-toggle" href="#" id="alertsDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-bell fa-fw"></i>
                    <span class="badge badge-danger">2</span>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="alertsDropdown">
                    <a class="dropdown-item" href="#">Informação</a>
                    <a class="dropdown-item" href="#">Outra Informação</a>
                </div>
            </li>
        </ul>
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="sidebar navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="/Home/Index">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Home/Graph">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Charts</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Home/Table">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Tables</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/Index">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/FuncoesOcupantes">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Funções x Quantidade Ocupantes</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/DistribuicaoFuncoes">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Distribuição  de  Cargos  Comissionados</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/Filiados">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Top servidores que trocaram de função</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/EvolucaoCargos">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Evolução  dos  Cargos</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Graficos/PartidosServidores">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Partido  Político  e  Servidores</span>
                </a>
            </li>
        </ul>
        <div id="content-wrapper">

            <div class="container-fluid">
                <!-- Breadcrumbs-->
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="index.html">Graphs</a>
                    </li>
                    <li class="breadcrumb-item active">Home Page</li>
                </ol>
                <!-- Page Content -->
                @RenderBody()
            </div>

            <!-- /.container-fluid -->
            <!-- Sticky Footer -->
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>© FG App @DateTime.Now.Year</span>
                    </div>
                </div>
            </footer>
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    @RenderSection("Scripts", required: false)
</body>
</html>

